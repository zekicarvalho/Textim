{
  "name": "notetron",
  "productName": "Notetron",
  "version": "0.1.0",
  "description": "A simple notepad desktop app built on Electron and Quill",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package": "asar pack notetron-linux-x64/resources/app notetron-linux-x64/resources/app.asar",
    "build": "electron-packager . notetron --ignore=node_modules/electron-* --platform=linux --arch=x64 --version=0.37.8"
  },
  "author": "Cody Pearce",
  "license": "ISC",
  "devDependencies": {
    "asar": "^0.11.0",
    "electron": "^1.4.13",
    "electron-packager": "^7.0.1"
  },
  "dependencies": {
    "jquery": "^3.4.1",
    "quill": "^0.20.1"
  }
}
